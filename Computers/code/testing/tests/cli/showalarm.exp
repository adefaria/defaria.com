set test_base "/dview/defaria_default/Tools/testing"

source "$test_base/bin/TestPkg.tcl"

catch {namespace import Test::*}

proc Setup {} {
  Start "show alarm"
}

proc Run {} {
  set Test::result 1

  set more 1
  send "show alarm all\r"

  while {$more} {
    expect {
      $Test::success {
        set Test::result 0
	set more 0
      }
      "Q to Quit" {
        send "Q\r"
      }
    }
  }
}

proc CleanUp {} {
  End
}

# Main
Setup
Run
CleanUp
